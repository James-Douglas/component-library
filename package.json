{
  "name": "@comparethemarketau/manor",
  "version": "0.0.0",
  "main": "./lib/index.js",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/pro-regular-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@fortawesome/fontawesome-pro": "^5.12.0",
    "@tippy.js/react": "^3.1.1",
    "prop-types": "^15.7.2",
    "react-app-polyfill": "^1.0.3",
    "tippy.js": "^5.1.1"
  },
  "scripts": {
    "build": "npx rollup -c rollup.config.js",
    "watch": "npx rollup -c rollup.config.js -w",
    "tdd": "jest --coverage --watchAll",
    "test": "cross-env JEST_SILENT_REPORTER_DOTS=true jest --reporters=jest-silent-reporter --coverage --watchAll",
    "test:ci": "cross-env CI=true jest",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "lint": "eslint --ext .js src/ --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test:ci",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx,ts,tsx}",
      "src/hooks/**/*.{js,jsx,ts,tsx}",
      "src/utils/**/*.{js,jsx,ts,tsx}"
    ],
    "roots": [
      "src",
      "config"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/fileTransformer.js",
      "^.+\\.js$": "babel-jest"
    },
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    }
  },
  "publishConfig": {
    "registry": "https://nexus.comparethemarket.com.au/repository/platform-packages/",
    "access": "public",
    "tag": "latest"
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/traverse": "7.7.2",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@semantic-release/commit-analyzer": "^6.3.3",
    "@semantic-release/release-notes-generator": "^7.3.5",
    "@storybook/addon-a11y": "^5.2.8",
    "@storybook/addon-actions": "^5.3.0-beta.14",
    "@storybook/addon-docs": "^5.3.0-beta.14",
    "@storybook/addon-knobs": "^5.3.0-beta.14",
    "@storybook/addon-links": "^5.3.0-beta.14",
    "@storybook/addon-notes": "^5.3.0-beta.14",
    "@storybook/addon-storysource": "^5.3.0-beta.14",
    "@storybook/addon-viewport": "^5.3.0-beta.14",
    "@storybook/addons": "^5.3.0-beta.14",
    "@storybook/react": "^5.3.0-beta.14",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.1.4",
    "@testing-library/react-hooks": "3.2.1",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "cross-env": "^6.0.0",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-react-app": "^5.0.2",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "2.2.0",
    "husky": "^3.0.5",
    "identity-obj-proxy": "^3.0.0",
    "import-index-generator": "^0.2.1",
    "jest": "^24.9.0",
    "jest-silent-reporter": "^0.1.2",
    "jest-static-stubs": "^0.0.1",
    "jest-styled-components": "^6.3.4",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "react-test-renderer": "^16.9.0",
    "rollup": "1.26.5",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-ignore-import": "^1.3.2",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-externals": "^2.1.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-smart-asset": "^2.0.0",
    "rollup-plugin-url": "^3.0.1",
    "semantic-release": "^15.13.24",
    "styled-components": "^4.4.1",
    "tailwindcss": "^1.1.2",
    "tailwindcss-typography": "^2.2.0"
  },
  "peerDependencies": {
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "styled-components": "^4.4.1"
  }
}
