{
  "name": "@comparethemarketau/manor",
  "version": "0.0.0",
  "dependencies": {
    "@craco/craco": "^5.5.0",
    "@tippy.js/react": "^2.2.3",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-app-polyfill": "^1.0.3",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1",
    "styled-jsx": "^3.2.2",
    "styled-jsx-plugin-postcss": "^2.0.1"
  },
  "scripts": {
    "build": "craco build",
    "tdd": "craco test --coverage --watchAll",
    "test": "cross-env JEST_SILENT_REPORTER_DOTS=true craco test --reporters=jest-silent-reporter --coverage --watchAll",
    "test:ci": "cross-env CI=true craco test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "lint": "eslint --ext .js src/ --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test:ci",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/stories/**/*.{js,jsx,ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    }
  },
  "publishConfig": {
    "registry": "https://nexus.comparethemarket.com.au/repository/npm-all/",
    "access": "public",
    "tag": "latest"
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@storybook/addon-actions": "^5.2.1",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-knobs": "^5.2.1",
    "@storybook/addon-links": "^5.2.0",
    "@storybook/addon-notes": "^5.2.1",
    "@storybook/addon-storysource": "^5.2.1",
    "@storybook/addon-viewport": "^5.2.1",
    "@storybook/addons": "^5.2.0",
    "@storybook/react": "^5.2.0",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.1.4",
    "@testing-library/react-hooks": "^2.0.1",
    "autoprefixer": "^9.6.1",
    "cross-env": "^6.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^3.0.5",
    "jest-silent-reporter": "^0.1.2",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "react-test-renderer": "^16.9.0",
    "semantic-release": "^15.13.24",
    "tailwindcss": "^1.1.2",
    "tailwindcss-typography": "^2.2.0"
  }
}
